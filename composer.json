{
  "name": "semaio/laravel-searchy",
  "description": "Laravel Searchy makes user driven searching easy with fuzzy search, basic string matching, Levenshtein Distance and more.",
  "keywords": [
    "laravel",
    "search",
    "fuzzy"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Lingham",
      "email": "tjlingham@gmail.com"
    },
    {
      "name": "Rouven Alexander Rieker",
      "email": "therouv@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^7.0"
  },
  "require-dev": {
    "graham-campbell/testbench": "^5.4",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "TomLingham\\Searchy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TomLingham\\Tests\\Searchy\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    },
    "laravel": {
      "providers": [
        "TomLingham\\Searchy\\SearchyServiceProvider"
      ],
      "aliases": {
        "Searchy": "TomLingham\\Searchy\\Facades\\Searchy"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
